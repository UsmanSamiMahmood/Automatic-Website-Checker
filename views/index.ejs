    <script>
        $(window).scroll(function(){
            $('nav').toggleClass('scrolled', $(this).scrollTop() > 50);
            if ($(".dropdown").hasClass("visibleDropdown")) {
                 $(".dropdown").toggleClass("visibleDropdown")
                 $(".dropdown").toggleClass("hiddenDropdown")
            }

            //$('#mainTitle').toggleClass('scrolled', $(this).scrollTop() > 50);
        });

        document.body.addEventListener('click', function() {
            if ($(".dropdown").hasClass("visibleDropdown")) {
                 $(".dropdown").toggleClass("visibleDropdown")
                 $(".dropdown").toggleClass("hiddenDropdown")
            }
        }, true); 

        $(".dropdown").toggleClass("hiddenDropdown")

        function toggleDropdown() {
            $(".dropdown").toggleClass("visibleDropdown")
            $(".dropdown").toggleClass("hiddenDropdown")
        }

        
    </script>
